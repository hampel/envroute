{
    "name": "hampel/envroute",
    "description": "Package for customising the Laravel environment based on route",
	"homepage" : "https://bitbucket.org/hampel/envroute",
	"authors": [{
		"name": "Simon Hampel",
		"email": "simon@hampelgroup.com"
	}],
	"support": {
		"issues": "https://bitbucket.org/hampel/envroute/issues",
		"source": "https://bitbucket.org/hampel/envroute/src"
	},
	"keywords" : [
		"laravel",
		"environment"
	],
	"license" : [
		"MIT"
	],
	"require": {
		"php": ">=5.5.0",
		"illuminate/support": "~5.0",
		"illuminate/filesystem": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "EnvRoute\\": "src/"
        }
    },
	"extra": {
		"branch-alias": {
			"dev-master": "0.3.x-dev"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
